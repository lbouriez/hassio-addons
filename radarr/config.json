{
  "name": "Radarr",
  "version": "0.1.2",
  "slug": "radarr_docker",
  "description": "Radarr add-on based on the prebuilt docker image from linuxserver",
  "url": "https://github.com/marcelveldt/hassio-addons-repo/radarr",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "ports": {
    "7878/tcp": 7878
  },
  "ports_description": {
    "7878/tcp": "Web interface"
  },
  "map": [
    "backup:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:7878]",
  "ssl": false,
  "ingress": false,
  "ingress_port": 7878,
  "ingress_entry": "radarr/",
  "boot": "auto",
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "options": {},
  "schema": {}
}